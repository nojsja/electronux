{
  "appId": "com.nojsja.electronux",
  "copyright": "nojsja",
  "productName": "electronux",
  "asar": false,
  "directories": {
    "buildResources": "resources/",
    "output": "build/"
  },
  "files": ["package.json", "index.js", "dist/", "app/", "node_modules/", "resources/*.*"],
  "linux": {
    "icon": "build/",
    "category": "System",
    "description": "A linux system tool powered by electron/react/mobx",
    "synopsis": "electronux",
    "target": ["zip", "deb"],
    "publish": {
      "provider": "github",
      "repo": "https://github.com/NoJsJa/electronux.git",
      "owner": "NoJsJa",
      "vPrefixedTagName": true,
      "host": "github.com",
      "protocol": "https",
      "token": "650eb5ae88fc8993e8c9b8030b811f734023e57a",
      "private": true,
      "releaseType": "release",
      "publishAutoUpdate": true
    }
  },
  "win": {
    "icon": "build/icon.ico",
    "target": [
      {
        "target": "zip"
      },
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    ]
  },
  "nsis": {
    "oneClick": false,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "./build/icon.ico",
    "uninstallerIcon": "./build/icon.ico",
    "installerHeaderIcon": "./build/icon.ico",
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true,
    "deleteAppDataOnUninstall": true,
    "shortcutName": "electronux"
  }
}
